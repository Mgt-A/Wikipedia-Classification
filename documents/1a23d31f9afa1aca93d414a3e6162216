<area href="/wiki/Animal" shape="rect" coords="362,109,395,116" alt="Animal" title="Animal" />
<area href="/wiki/Fungus" shape="rect" coords="382,119,404,129" alt="Fungus" title="Fungus" />
<area href="/wiki/Plant" shape="rect" coords="382,143,407,150" alt="Plant" title="Plant" />
<area href="/wiki/Ciliate" shape="rect" coords="382,158,412,165" alt="Ciliate" title="Ciliate" />
