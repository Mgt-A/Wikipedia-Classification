<area shape="rect" href="/wiki/Aluminum_PowerBook_G4" coords="616,175,724,195" title="Aluminum PowerBook G4" />
<area shape="rect" href="/wiki/Aluminum_PowerBook_G4" coords="616,154,729,175" title="Aluminum PowerBook G4" />
<area shape="rect" href="/wiki/PowerBook_G4" coords="551,154,615,175" title="PowerBook G4" />
<area shape="rect" href="/wiki/iBook_G4" coords="639,196,720,216" title="iBook G4" />
<area shape="rect" href="/wiki/PowerBook_G3_(Lombard/Pismo)" coords="498,154,562,175" title="PowerBook G3 (Lombard/Pismo)" />
<area shape="rect" href="/wiki/iBook_(white)" coords="562,196,670,216" title="iBook (white)" />
<area shape="rect" href="/wiki/PowerBook_G3" coords="450,154,514,175" title="PowerBook G3" />
<area shape="rect" href="/wiki/PowerBook_with_PowerPC" coords="378,154,447,175" title="PowerBook with PowerPC" />
<area shape="rect" href="/wiki/iBook" coords="503,196,567,216" title="iBook" />
<area shape="rect" href="/wiki/PowerBook_500" coords="338,154,407,175" title="PowerBook 500" />
<area shape="rect" href="/wiki/PowerBook_2400c" coords="434,175,498,195" title="PowerBook 2400c" />
