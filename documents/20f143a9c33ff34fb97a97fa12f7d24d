<th colspan="2" style="background:#ccccff;">Web</th>
<th style="background:#ccccff;" rowspan="2">Production<br />
Premium</th>
<th style="background:#ccccff;" rowspan="2">Master<br />
Collection</th>
</tr>
<tr>
<th style="background:#ccccff;">Standard</th>
<th style="background:#ccccff;">Premium</th>
<th style="background:#ccccff;">Standard</th>
<th style="background:#ccccff;">Premium</th>
</tr>
