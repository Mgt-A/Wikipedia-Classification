<area href="/wiki/Sai_Kung_District" shape="poly" coords="353,78,351,79,352,81,351,83,353,88,352,96,346,101,344,101,343,99,340,99,336,94,333,96,326,93,326,91,320,88,318,90,318,94,309,95,304,103,301,103,299,106,290,106,287,108,289,112,285,113,280,120,275,123,273,123,271,127,271,131,270,131,271,145,274,145,277,152,282,153,282,157,283,157,283,160,280,162,283,166,284,170,282,170,282,175,289,168,287,163,290,164,293,165,295,167,296,172,297,175,296,178,295,183,300,181,303,188,305,185,314,191,318,187,324,182,316,180,312,182,309,176,310,173,315,175,321,170,316,161,311,164,309,154,299,153,296,136,289,133,294,123,300,134,303,130,298,123,301,116,304,110,310,111,319,114,321,109,326,109,326,116,334,118,333,126,338,137,343,133,346,145,350,141,350,135,356,134,359,128,359,121,362,120,363,126,366,124,366,111,358,107,355,107,354,104,359,98,363,93,368,92,372,97,375,96,373,91,370,87,372,82,366,80,359,82" alt="Sai Kung" title="Sai Kung" />
<area href="/wiki/Sai_Kung_District" shape="poly" coords="315,118,311,120,308,125,312,131,317,132,320,140,323,139,326,150,331,150,333,145,329,140,330,132,325,127,324,120,320,118" alt="Sai Kung" title="Sai Kung" />
<area href="/wiki/Sai_Kung_District" shape="poly" coords="311,193,305,196,304,201,308,205,312,203,313,198" alt="Sai Kung" title="Sai Kung" />
<area href="/wiki/Sai_Kung_District" shape="poly" coords="311,147,311,150,313,153,315,153,315,151" alt="Sai Kung" title="Sai Kung" />
<area href="/wiki/Sai_Kung_District" shape="poly" coords="343,151,341,157,345,157,347,151" alt="Sai Kung" title="Sai Kung" />
