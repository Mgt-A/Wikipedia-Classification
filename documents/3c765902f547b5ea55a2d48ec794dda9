<area href="/wiki/Bahamian_Dialect" shape="poly" coords="143,138,139,151,159,162,168,153,159,140" alt="Bahamian Dialect" title="Bahamian Dialect" />
<area href="/wiki/American_English" shape="poly" coords="69,85,135,86,154,93,156,100,190,87,194,92,181,99,157,125,143,133,142,147,139,149,127,139,100,149,38,122,55,87" alt="American English" title="American English" />
<area href="/wiki/Saint_Helena" shape="circle" coords="320,261,20" alt="Saint Helena" title="Saint Helena" />
<area href="/wiki/Guyana#Languages" shape="rect" coords="176,193,202,224" alt="Guyana#Languages" title="Guyana#Languages" />
