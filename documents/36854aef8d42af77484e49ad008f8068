<td rowspan="3">{0, <i>a</i>, 2<i>a</i>, <i>b</i>, <i>b</i>+<i>a</i>, <i>b</i>+2<i>a</i>}&#160;/&#160;{0, <i>b</i>}&#160;= {{0, <i>b</i>}, {<i>a</i>, <i>b</i>+<i>a</i>}, {2<i>a</i>, <i>b</i>+2<i>a</i>}}</td>
