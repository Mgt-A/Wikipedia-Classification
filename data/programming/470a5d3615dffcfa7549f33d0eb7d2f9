<span class="sc3"><span class="re1">&lt;?xml-stylesheet</span> <span class="re0">href</span>=<span class="st0">"chrome://global/skin/"</span> <span class="re0">type</span>=<span class="st0">"text/css"</span><span class="re2">?&gt;</span></span>
 
<span class="sc3"><span class="re1">&lt;window</span> <span class="re0">id</span>=<span class="st0">"vbox example"</span> <span class="re0">title</span>=<span class="st0">"Example"</span>
